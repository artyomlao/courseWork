<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginViewController">
   <children>
      <TextField fx:id="loginField" layoutX="191.0" layoutY="159.0" prefHeight="25.0" prefWidth="219.0" promptText="Логин" />
      <Button onAction="#authorise" fx:id="authoriseButton" layoutX="276.0" layoutY="251.0" mnemonicParsing="false" text="Войти" />
      <Hyperlink fx:id="loginSignUpHyperlink" layoutX="204.0" layoutY="287.0" onAction="#register" text="Нет аккаунта? Зарегистрируйтесь" />
      <PasswordField fx:id = "passwordField" layoutX="191.0" layoutY="200.0" prefHeight="25.0" prefWidth="219.0" promptText="Пароль" />
   </children>
</AnchorPane>
